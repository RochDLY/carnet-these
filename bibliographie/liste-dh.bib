	journal = {LHT Fabula},
	author = {Gervais, Bertrand},
	month = jan,
	year = {2016},
	note = {Publisher: LHT Fabula / Équipe de recherche Fabula},
	file = {Snapshot:/home/user/Zotero/storage/XID74GTL/gervais.html:text/html},
}

